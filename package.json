{
  "name": "quad-hexer",
  "private": true,
  "version": "0.1.0",
  "description": "A library for converting between quad and hex representations of geocodes.",
  "keywords": [
    "quadkey",
    "spatial-id",
    "geocode",
    "encode",
    "decode"
  ],
  "author": "sanak",
  "homepage": "https://github.com/sanak/quad-hexer",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:sanak/quad-hexer.git"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/quad-hexer.umd.js",
  "module": "./dist/quad-hexer.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/quad-hexer.es.js",
    "require": "./dist/quad-hexer.cjs.js"
  },
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "build": "vite build && tsc",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@reearth/spatial-id-sdk": "^0.0.0",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^22.14.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "maplibre-gl": "^5.3.1",
    "prettier": "^3.5.3",
    "quadkey-tilemath": "^0.1.2",
    "s2-geometry": "^1.2.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6",
    "vitest": "^3.1.1"
  }
}